{
  "ignorePatterns": [
    {
      "pattern": "http(s)?://\\d+\\.\\d+\\.\\d+\\.\\d+"
    },
    {
      "pattern": "http(s)?://localhost"
    },
    {
      "pattern": "http(s)?://example.com"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "https://github.com/open-telemetry/opentelemetry-collector(-contrib)?/releases/tag/vXX.YY.ZZ"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": " \\(#(?<prnumber>\\d+)\\)$",
      "replacement": "https://github.com/Dynatrace/dynatrace-otel-collector/pull/$<prnumber>"
    }
  ],
  "aliveStatusCodes": [429, 200]
}
