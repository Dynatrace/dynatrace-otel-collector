{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base"
    ],
    "regexManagers": [
        {
          "fileMatch": ["^manifest.yaml$"],
          "matchStrings": ["- gomod: (?<depName>.*?) (?<currentValue>.*)\\n"],
          "datasourceTemplate": "go"
        }
    ]
  }
